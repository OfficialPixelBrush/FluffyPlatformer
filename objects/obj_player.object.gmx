            <string>/// Vertical

/*
// Not exceed speed
if vspd &gt; maxvspd {
    vspd = maxvspd;
};

if vspd &lt; (maxvspd*(-1)) {
    vspd = (maxvspd*(-1));
};
*/

if jumping = true {
    jumpanimtimer ++;    
} else {
    jumpanimtimer = 0;
};

// Jump Animation
if jumping = true {
    if runanimtimer &lt; 4 {
        runanimtimer ++;
        if runanimtimer &lt; 2 {
            sprite_index = spr_fiona_run_idle;
            image_index=0;
        };        
        if runanimtimer &gt; 2 {
            sprite_index = spr_fiona_run_idle;
            image_index=1;            
        };
    } else if runanimtimer &gt;= 4 {
        sprite_index = spr_fiona_idle;
    };
};

// Falling
if jumping = false {
    if (vspd &lt; maxvspd) vspd += global.grav; 
};

// Vertical Collission
if (place_meeting(x,y+vspd,obj_solid))
{
while(!place_meeting(x,y+sign(vspd),obj_solid))
{
y += sign(vspd);
}
vspd = 0;
}
y+=vspd;

// Jumping
if (key_jump &gt; 0 &amp;&amp; jumping = false) {
    jumping = true;
} else {
    jumping = false;
};

if jumping = true &amp;&amp; place_meeting(x,y+1, obj_solid) {
    vspd-=vacceleration*4;
    jumptimer++;    
};

if jumptimer &gt;= maxjumptimer {
    jumping = false;
};
